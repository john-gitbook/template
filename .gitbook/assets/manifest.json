{
  "manifest_version": 3,
  "name": "GitBook Extension",
  "version": "2.0",
  "description": "Extract GitBook links and IDs with easy copy functionality.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "GitBook Extension"
  }
}
